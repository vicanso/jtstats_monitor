@import 'nib'
html, body
  font-family "Helvetica Neue", Helvetica, Arial, sans-serif
  font-size 14px
  line-height 1.42857143
  color #333

.hidden
  display none
.clearfix
  clearfix()
.error
  color #a94442
  background-color #f2dede
  border-color #ebccd1
  border-radius 3px
.warning
  color #8a6d3b
  background-color #fcf8e3
  border-color #faebcc
  border-radius 3px
@import './grid'

